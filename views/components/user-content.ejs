<table class="ui celled table">
  <thead>
    <tr>
    	<th>姓名</th>
    	<th>密码</th>
    	<th>性别</th>
    	<th>是否管理员</th>
    	<th>简介</th>
    	<th>操作</th>
  	</tr>
  </thead>
  <tbody>
    
    <% users.forEach(function (user) { %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.password %></td>
      <td><%= user.gender %></td>
      <td><%= user.isAdmin %></td>
      <td><%= user.bio %></td>
      <td>
      <% if(user.isAdmin=="false"){ %>
      	<a href="/users/<%= user._id %>/remove">删除</a>
      <% } else{%>
      删除
      <% }%>
      </td>
    </tr>
    <% }) %>
    
  </tbody>
</table>